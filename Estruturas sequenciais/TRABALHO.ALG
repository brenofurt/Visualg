Algoritmo "Consumo"

   

Var


    Distancia, Gasto, ConsumoMedio : real
   
   


Inicio


   escreva("Qual a distância percorrida: ")
   leia(Distancia)
   escreva("Qual o valor gasto em combustível: ")
   leia(Gasto)

   ConsumoMedio <- distancia / gasto
   escreval
   escreva("O consumo médio do carro corresponde a: ", ConsumoMedio:8:3, " km")


Fimalgoritmo